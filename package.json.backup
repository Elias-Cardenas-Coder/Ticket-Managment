{
  "name": "nextapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:backend": "ts-node --project tsconfig.backend.json backend/src/index.ts",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start:backend": "node backend/dist/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "seed": "ts-node --project tsconfig.backend.json prisma/seed.ts",
    "lint": "eslint"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "i18next": "^25.7.1",
    "next": "16.0.4",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "prisma": "^5.22.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "^16.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
